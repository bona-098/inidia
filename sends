$data->transform(function ($item) {
    // Jika guest bukan array, ubah dengan json_decode
    if (!is_array($item->guest)) {
        $item->guest = json_decode($item->guest, true) ?? [];
    }
    // Jika family bukan array, ubah dengan json_decode
    if (!is_array($item->family)) {
        $item->family = json_decode($item->family, true) ?? [];
    }
    return $item;
});
