{
    dataField: "roomName",
    lookup: {
        dataSource: function(options) {
            const currentSector = formInstance.option("formData").sector; // Ambil sector user
            return allRooms
                .filter(room => room.sector === currentSector) // Hanya room dalam sector yang sama
                .map(room => ({ id: room.roomName, name: room.roomName }));
        },
        displayExpr: "name",
        valueExpr: "id"
    }
}
